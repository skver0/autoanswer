{
  "manifest_version": 2,
  "name": "AutoAnswer",
  "version": "1.0",
  "description": "Automatically answer questions on specific pages",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
